{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "module": "esnext",
    "target": "es5",
    "strict": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "exclude": ["node_modules"],
  "include": ["src", "data"]
}
